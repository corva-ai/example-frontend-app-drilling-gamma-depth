{
  "name": "@corva/ui",
  "version": "0.30.0",
  "description": "Shared components/utils for Corva ui projects",
  "keywords": [
    "corva-ui"
  ],
  "update": "update",
  "main": "./dist",
  "module": "./src/index.js",
  "scripts": {
    "build": "yarn generate-css-themes && ./scripts/build.sh production",
    "build-watch": "yarn generate-css-themes && ./scripts/build.sh development -w",
    "get-changelog": "conventional-changelog -r 2 -p angular",
    "lint": "eslint --cache ./src/",
    "lint-staged": "lint-staged",
    "release": "git fetch --tags && git add -A && standard-version -a",
    "start": "yarn generate-css-themes && cross-env NODE_ENV=local rollup -c -w",
    "test": "jest",
    "test-coverage": "jest --watchAll --coverage && http-serve coverage",
    "test-watch": "jest --watch",
    "generate-css-themes": "node ./scripts/generateCssThemesVariables.mjs"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "pre-commit": [
    "lint-staged"
  ],
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ]
  },
  "standard-version": {
    "skip": {
      "tag": true
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "./**/*.{js,jsx,mjs}"
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ],
    "moduleNameMapper": {
      "~(.*)": "<rootDir>/src/$1",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "<rootDir>/jest/setEnvVars.js"
    ],
    "testMatch": [
      "**/__tests__/**/?(*.)+(spec|test).[jt]s?(x)"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.mjs$": "babel-jest",
      "^.+\\.svg$": "<rootDir>/jest/svgTransform.js"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "mjs",
      "node"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-react-jsx": "^7.12.11",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@corva/eslint-config-browser": "^0.0.4",
    "@date-io/moment": "1.3.13",
    "@material-ui/core": "4.11.2",
    "@material-ui/icons": "4.9.1",
    "@material-ui/lab": "4.0.0-alpha.57",
    "@material-ui/pickers": "3.2.10",
    "auth0-js": "^9.14.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "^26.3.0",
    "babel-loader": "8.0.6",
    "babel-plugin-file-loader": "^2.0.0",
    "babel-plugin-root-import": "^6.5.0",
    "chalk": "4.1.1",
    "chroma-js": "1.4.1",
    "classnames": "2.2.6",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "6.1.1",
    "corva-convert-units": "1.10.25",
    "css-loader": "3.4.2",
    "dotenv": "^10.0.0",
    "emoji-mart": "3.0.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-jest-dom": "^3.2.3",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-testing-library": "^3.9.2",
    "express": "4.17.1",
    "file-extension": "4.0.5",
    "file-saver": "1.3.8",
    "fs": "0.0.1-security",
    "highcharts": "8.1.2",
    "highcharts-multicolor-series": "2.2.7",
    "highcharts-react-official": "2.2.2",
    "html-webpack-plugin": "4.0.0-beta.11",
    "html2canvas": "1.0.0-alpha.12",
    "image-extensions": "1.1.0",
    "immutable": "3.8.2",
    "jest": "^26.4.1",
    "jsona": "1.5.1",
    "lodash": "4.17.20",
    "mapbox.js": "3.2.1",
    "mini-css-extract-plugin": "0.9.0",
    "mobile-detect": "1.4.3",
    "moment": "2.23.0",
    "moment-duration-format": "1.3.0",
    "moment-timezone": "0.5.23",
    "node-fetch": "^2.6.1",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "path": "0.12.7",
    "postcss": "8.2.4",
    "postcss-import": "14.0.0",
    "postcss-loader": "4.2.0",
    "postcss-preset-env": "6.7.0",
    "prettier": "^2.1.2",
    "prop-types": "15.7.2",
    "query-string": "4.3.4",
    "react": "17.0.1",
    "react-color": "2.18.1",
    "react-dom": "17.0.1",
    "react-grid-layout": "0.18.3",
    "react-image-lightbox": "5.1.0",
    "react-infinite-scroller": "1.2.4",
    "react-mentions": "3.3.1",
    "react-number-format": "^4.7.3",
    "react-player": "1.9.3",
    "react-router": "3.2.1",
    "react-s3-uploader": "4.8.0",
    "react-sizeme": "2.5.2",
    "react-sortable-hoc": "0.6.8",
    "react-virtualized": "9.21.2",
    "react-virtualized-auto-sizer": "1.0.2",
    "rollup-plugin-copy": "^3.4.0",
    "slate": "^0.62.0",
    "slate-react": "^0.62.0",
    "socket.io-client": "2.2.0",
    "sql-parser": "0.5.0",
    "style-loader": "1.0.0",
    "svg-url-loader": "6.0.0",
    "terser-webpack-plugin": "4.2.3",
    "use-tiny-state-machine": "^0.0.2",
    "uuid": "3.4.0",
    "video-extensions": "1.1.0",
    "webpack": "4.41.5",
    "webpack-bundle-analyzer": "3.9.0",
    "webpack-cli": "3.3.6",
    "webpack-dev-server": "3.10.2",
    "webpack-manifest-plugin": "2.2.0",
    "webpack-merge": "5.7.2"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-image": "^2.0.6",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "babel-plugin-external-helpers": "^6.22.0",
    "conventional-changelog-cli": "^2.1.0",
    "cross-env": "^7.0.3",
    "identity-obj-proxy": "^3.0.0",
    "lint-staged": "^10.5.4",
    "node-sass": "^6.0.1",
    "pre-commit": "1.2.2",
    "rollup": "^2.28.2",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-css-bundle": "^1.0.4",
    "rollup-plugin-css-only": "^2.1.0",
    "rollup-plugin-includepaths": "^0.2.4",
    "rollup-plugin-inject-process-env": "^1.3.1",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-node-externals": "^2.2.0",
    "rollup-plugin-postcss": "4.0.0",
    "rollup-plugin-rename-node-modules": "^1.0.1",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^4.1.1",
    "standard-version": "^9.0.0"
  },
  "engines": {
    "node": ">=10"
  }
}

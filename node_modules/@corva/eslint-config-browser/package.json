{
  "name": "@corva/eslint-config-browser",
  "version": "0.0.4",
  "description": "Defines global linting rules for Corva.AI frontend projects",
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "license": "UNLICENSED",
  "author": "Corva.ai",
  "main": "eslint/index.js",
  "files": [
    "eslint/index.js",
    "prettier/index.js"
  ],
  "scripts": {
    "get-changelog": "conventional-changelog -r 2 -p angular",
    "lint": "eslint .",
    "release": "git add -A && standard-version -a",
    "test": "exit 0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@babel/core": "7.12.10",
    "@babel/eslint-parser": "7.12.17",
    "eslint": "^7.22.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.4",
    "@commitlint/config-conventional": "^8.3.4",
    "conventional-changelog-cli": "^2.0.34",
    "husky": "^4.2.5",
    "pretty-quick": "^2.0.0",
    "standard-version": "^8.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
